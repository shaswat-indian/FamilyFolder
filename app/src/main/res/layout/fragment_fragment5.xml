            android:textColor="@android:color/black"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/lQ"
            app:layout_constraintStart_toEndOf="@+id/lQ"
            app:layout_constraintTop_toTopOf="@+id/lQ" />

        <TextView
            android:id="@+id/lR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="R. Waste segregation"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/lQ"
            app:layout_constraintTop_toBottomOf="@+id/lQ" />

        <Spinner
            android:id="@+id/fR"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:background="@drawable/spinner_style"
            app:layout_constraintBottom_toBottomOf="@+id/lR"
            app:layout_constraintStart_toEndOf="@+id/lR"
            app:layout_constraintTop_toTopOf="@+id/lR" />

        <TextView
            android:id="@+id/lS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="S. Any other observations?"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/lR"
            app:layout_constraintTop_toBottomOf="@+id/lR" />

        <EditText
            android:id="@+id/fS"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:background="@drawable/spinner_style"
            android:inputType="textPersonName"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/lS"
            app:layout_constraintStart_toEndOf="@+id/lS"
            app:layout_constraintTop_toTopOf="@+id/lS" />

    </android.support.constraint.ConstraintLayout>
</FrameLayout>
